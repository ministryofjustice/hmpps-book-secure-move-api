{
  "ProfileIdentifier": {
    "type": "object",
    "required": [
      "id",
      "value",
      "indentifier_type"
    ],
    "properties": {
      "id": {
        "type": "string",
        "format": "uuid",
        "example": "7cc9c528-3301-4d69-f200-69e482ce1ed8"
      },
      "value": {
        "type": "string",
        "example": "CR451905001"
      },
      "identifier_type": {
        "type": "string",
        "enum": ["pnc_number", "cro_number", "prison_number", "niche_reference", "athena_reference"]
      }
    }
  }
}
