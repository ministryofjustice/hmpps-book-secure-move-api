{
  "HealthAlert": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "format": "uuid",
        "example": "7cc9c528-3301-4d69-f200-69e482ce1ed8"
      },
      "type": {
        "type": "string",
        "example": "health_alerts"
      },
      "attributes": {
        "type": "object",
        "properties": {
          "alert_type": {
            "type": "string",
            "example": "M"
          },
          "alert_code": {
            "type": "string",
            "example": "MAS"
          },
          "alert_date": {
            "type": "string",
            "example": "2019-05-06"
          },
          "alert_expiry_date": {
            "type": "string",
            "example": "2019-06-06"
          },
          "description": {
            "oneOf": [
              { "type": "string" },
              { "type": "null" }
            ],
            "example": "Asthmatic"
          },
          "comments": {
            "oneOf": [
              { "type": "string" },
              { "type": "null" }
            ],
            "example": "Needs an inhaler"
          }
        }
      }
    }
  }
}
