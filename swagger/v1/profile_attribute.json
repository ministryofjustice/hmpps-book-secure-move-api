{
  "ProfileAttribute": {
    "type": "object",
    "required": [
      "id",
      "description",
      "profile_attribute_type_id"
    ],
    "properties": {
      "id": {
        "type": "string",
        "format": "uuid",
        "example": "7cc9c528-3301-4d69-f200-69e482ce1ed8",
        "description": "The unique identifier (UUID) of this profile attribute"
      },
      "date": {
        "type": "string",
        "example": "2019-05-06"
      },
      "expiry_date": {
        "type": "string",
        "example": "2019-06-06"
      },
      "description": {
        "type": "string",
        "example": "Asthmatic"
      },
      "comments": {
        "oneOf": [
          { "type": "string" },
          { "type": "null" }
        ],
        "example": "Needs an inhaler"
      },
      "profile_attribute_type_id": {
        "type": "string",
        "format": "uuid",
        "example": "49fa81ee-3301-4d69-f200-69e482ce1ed8"
      }
    }
  }
}
