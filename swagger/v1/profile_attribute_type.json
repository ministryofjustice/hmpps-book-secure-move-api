{
  "ProfileAttributeType": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "format": "uuid",
        "example": "3852df1a-3301-4d69-f200-69e482ce1ed8"
      },
      "type": {
        "type": "string",
        "example": "profile_attribute_types"
      },
      "attributes": {
        "type": "object",
        "properties": {
          "profile_attribute_category": {
            "type": "String",
            "enum": ["health", "risk", "court_information"]
          },
          "user_type": {
            "type": "String",
            "enum": ["prison", "police"]
          },
          "alert_type": {
            "type": "String",
            "example": "M"
          },
          "alert_code": {
            "type": "String",
            "example": "MSI"
          },
          "type_description": {
            "type": "String",
            "example": "Medical"
          },
          "description": {
            "type": "String",
            "example": "Sight Impaired"
          }
        }
      }
    }
  }
}
