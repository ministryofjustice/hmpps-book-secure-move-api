{
  "Supplier": {
    "type": "object",
    "required": [
      "id",
      "type",
      "attributes"
    ],
    "properties": {
      "id": {
        "type": "string",
        "format": "uuid",
        "example": "3fa9c528-3301-4d69-f200-69e482ce1ed8",
        "description": "The unique identifier (UUID) of this object"
      },
      "type": {
        "type": "string",
        "example": "suppliers",
        "description": "The type of this object - always `supplier`"
      },
      "attributes": {
        "type": "object",
        "required": [
          "name",
          "key"
        ],
        "properties": {
          "name": {
            "type": "string",
            "example": "GEOAmey",
            "description": "Human readable name of the supplier"
          },
          "key": {
            "type": "string",
            "example": "geoamey",
            "description": "Machine readable unique key string attribute"
          }
        }
      }
    }
  }
}
