[ai]
  instrumentation_key = '<%= ENV['APPINSIGHTS_INSTRUMENTATION_KEY'] %>'
  enabled = true

[[middleware]]
  name = 'AppInsights::ExceptionHandling'
  enabled = false

[[middleware]]
  name = 'AppInsights::TrackRequest'
  enabled = true

  [middleware.initialize]
    instrumentation_key = '<%= ENV['APPINSIGHTS_INSTRUMENTATION_KEY'] %>'
    prefixes_to_ignore = ['/ping', '/api-docs/', '/health']
    buffer_size = 250
    send_interval = 10
