[ai]
  instrumentation_key = '<%= ENV['APPINSIGHTS_INSTRUMENTATION_KEY'] %>'
  enabled = true

[[middleware]]
  name = 'AppInsights::ExceptionHandling'
  enabled = false

[[middleware]]
  name = 'ApplicationInsights::Rack::TrackRequest'
  enabled = true

  [middleware.initialize]
    instrumentation_key = '<%= ENV['APPINSIGHTS_INSTRUMENTATION_KEY'] %>'
    buffer_size = 250
    send_interval = 10
