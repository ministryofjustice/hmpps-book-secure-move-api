[ai]
  instrumentation_key = '<%= ENV.fetch('APPINSIGHTS_INSTRUMENTATION_KEY') %>'
  enabled = true

[[middleware]]
  name = 'AppInsights::ExceptionHandling'
  enabled = true

[[middleware]]
  name = 'ApplicationInsights::Rack::TrackRequest'
  enabled = true

  [middleware.initialize]
    buffer_size = 250
    send_interval = 30
